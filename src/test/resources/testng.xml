<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="UI Regression Suite">
    <listeners>
        <listener class-name="core.AnnotationTransformer"/>
        <listener class-name="core.DataProviderRetryListener"/>
    </listeners>
    <!-- Login Test: runs first, sequentially -->
    <test name="LoginTest" parallel="false">
        <classes>
            <class name="UnbxdTests.testNG.login.LoginTest"/>
        </classes>
    </test>

    <!-- All other tests: run in parallel by class -->
    <test name="ParallelTests" parallel="classes" thread-count="8">
        <classes>
            <class name="UnbxdTests.testNG.consoleui.CoreAlgorithmsTest"/>
            <class name="UnbxdTests.testNG.consoleui.CustomAlgorithmsTest"/>
            <class name="UnbxdTests.testNG.consoleui.topsellersCustomAlgoTest"/>
            <class name="UnbxdTests.testNG.consoleui.ExpTest"/>
            <class name="UnbxdTests.testNG.consoleui.BoutiqueExperienceTest"/>
            <class name="UnbxdTests.testNG.consoleui.JourneyExperienceTest"/>
            <class name="UnbxdTests.testNG.consoleui.broadCastPageTest"/>
            <class name="UnbxdTests.testNG.consoleui.DHBoutiqueTemplateTest"/>
            <class name="UnbxdTests.testNG.consoleui.DHexitIntentTemplatetest"/>
            <class name="UnbxdTests.testNG.consoleui.DHstandardWidgettemplateTest"/>
            <class name="UnbxdTests.testNG.consoleui.DVBoutiqueTemplateTest"/>
            <class name="UnbxdTests.testNG.consoleui.DVexitIntentTemplateTest"/>
            <class name="UnbxdTests.testNG.consoleui.DVstandardWidgetTemplateTest"/>
            <class name="UnbxdTests.testNG.consoleui.MHboutiqueTemplateTest"/>
            <class name="UnbxdTests.testNG.consoleui.MHinstagramLikeBehaaviorTemplateTest"/>
            <class name="UnbxdTests.testNG.consoleui.MHstandardWidgettemplateTest"/>
            <class name="UnbxdTests.testNG.consoleui.MHtinderLikeBehaviorTemplateTest"/>
            <class name="UnbxdTests.testNG.consoleui.MVboutiqueTemplateTest"/>
            <class name="UnbxdTests.testNG.consoleui.MVinstagramLikeBehaviorTemplateTest"/>
            <class name="UnbxdTests.testNG.consoleui.MVstandardWidgetTemplateTest"/>
            <class name="UnbxdTests.testNG.consoleui.MVtinderLikeBehaviorTemplateTest"/>
        </classes>
    </test>
</suite> 